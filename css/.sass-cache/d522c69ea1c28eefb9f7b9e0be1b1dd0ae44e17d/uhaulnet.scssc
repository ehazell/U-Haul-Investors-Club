:@template"¯,/* Variables */

$mastheadbg: #7a7a7a;

$headerbg: #d9d9d9;
$headercolor:#666;

$fontcolor: #333;
$linkcolor: #496d9b;
$actioncolor: #f1d45e;

$green: #afd558;
$orange: #fb7700;

/* stuff to get rid of */
$disabledcolor: #a1a1a1;

$mobile: 40em;
$tablet: 60em;

/* Mixins */
@mixin no_select {
	-webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
	user-select: none;
}
@mixin button_gradient($basecolor){

  $topcolor: adjust-color($basecolor, $lightness:10);
  
  background: -moz-linear-gradient(top, $topcolor 0%, $basecolor 100%); /* FF3.6+ */
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,$topcolor), color-stop(100%,$basecolor)); /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(top, $topcolor 0%,$basecolor 100%); /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(top, $topcolor 0%,$basecolor 100%); /* Opera 11.10+ */
  background: -ms-linear-gradient(top, $topcolor 0%,$basecolor 100%); /* IE10+ */
  background: linear-gradient(top bottom, $topcolor 0%,$basecolor 100%); /* W3C */
}

/* Imports */
@import "foundation.scss";
@import "//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css";
/* Layout */
html, body { margin: 0; }
.content { 
	min-height: 100%;
	margin-bottom: -5em;
	&:after {
		display: block;
		content: "";
		height: 5em;
	}
}
.row { max-width: none; }

/* Header */
body .content > header#masthead {
	background: $mastheadbg;
	border-bottom:4px solid $orange;
	margin-bottom: 1em;
	position:relative;
	padding:0;
	
	.menu_button { 
		color: #fff;
		padding: .75em 1.5em .75em 1em;
		margin: 0;
		font-size: 1em;
		line-height:1.6;
		position:absolute;
		top:0;
		left:0;
		
		img{
		  vertical-align: sub;
		}
		&:hover{
		 background: adjust-color($headercolor, $lightness:20);
	  }
	}
	a{
	  text-decoration:none;
	  color:#fff;
	}
	form{
	  float:right;
	  margin:0;
	}
	.row{
		  background: adjust-color($headercolor, $lightness:40);
	}
	.popout{
	  background: adjust-color($headercolor, $lightness:40);
	  position:absolute;
	  right:.5em;
	  top:3.1em;
	  max-width:28%;
	  z-index:50;
	}
	.search{
	  display:none;
	  padding: .5em;
	  input{
	    margin:0;
	    height: 1.5em;
	  }
	  label{
	  margin:0;
	  width:auto;
	  }
	  input[type="submit"]{
	    min-width:0;
	    vertical-align:top;
	    margin-top:1px;
	  }
	}
	.account, .bookmark_link{
	   display:none;
	   padding:.5em;
	   margin:0;
	   li{
	     margin-left:0;
	     margin-right:.5em;
	     display:inline;
	     color: $headercolor;
	     a{
	       color: inherit;
	       text-decoration:underline;
	     }
	     &.username{
	       font-style:italic;
	     }
	     &.signout{
	       white-space:nowrap;
	     }
	   }
	}
	nav {
    color: #fff;
    &.menu {
      margin-right:12em;
      margin-left:8em;
    }
    ul {
      display: inline;
      padding: 1em 0;
      margin: 0;
      list-style-type: none;
      li {
        display: inline-block;
        margin:0;
        &.appname { 
          margin-right: 1em;
          border-right: 1px solid #fff;
        }
        &:last-of-type { margin-right: 0; }
        a, a:link, a:visited, a:hover, button, button:hover{
          display: inline-block;
          padding: .75em 1em;
          color: #fff;
          height: 100%;
          width: 100%;
          min-width:0;
          font-weight:bold;
          background:transparent;
          border:0;
          margin:0;
          box-shadow:none;
          font-size:inherit;
          line-height:1.6;
          &.active { background: adjust-color($headercolor, $lightness:20)}
          &:active { color: #d9d9d9; }
          &.down { color: #d9d9d9; }
        }
        a:hover{background: adjust-color($headercolor, $lightness:20)}
        button:hover{background: adjust-color($headercolor, $lightness:20)}
        button.active{
           background:transparent;
           :after{
             content:"\25B2";
             position:absolute;
             bottom:-.5em;
             left:.9em;
             color: adjust-color($headercolor, $lightness:40);
             
           }
        }
      }
    }
    &.icons{
      position:absolute;
      top:0; 
      right:0;
      ul li a{
        font-weight:normal;
      }
    }
  }
}


@media screen and (max-width:$tablet){
	body .content > header#masthead {
		.menu_button {
			padding: 1em 0.5em 1em 0;
			display: block;
			float:none;
			line-height:1.1;
			color:#fff;
			position:static;
			&:hover{
			  background:transparent;
			}
			.nav_button_bar {
				display: block;
				float:left; 
				clear:left;
				margin-right:.5em;
				width: 1em;
				height: 3px;
				margin-bottom: 0.15em;
				background: #FFF;
				
				-moz-transition:width .5s;
				-webkit-transition:width .5s;
				-o-transition:width .5s;
				transition:width .5s;
				
			}
			&.open .nav_button_bar{
			  width:.5em;
			}
			img{
			  vertical-align:top;
			  margin-top:-2px;
			  margin-right:.25em;
			}
		}
		.print_button{
		  display:none;
		}
		.popout{
	    max-width:none;
	    ul{
	      border-bottom:1px dotted $headercolor;
	    }
	  }
	  .search input[type="submit"]{
	    font-size:1.5em;
	  }
	  nav.menu { 
      left:-75%;
      width: 75%;
      position: absolute;
      z-index: 100;
      margin:0;
      
      -moz-transition:left .25s;
      -webkit-transition:left .25s;
      -o-transition:left .25s;
      transition:left .25s;
      
      ul {
        padding: 0;
        li{
          width:100%;
          a{
            background: adjust-color($headercolor, $lightness:-20);
            width: 100%;
            display: block;
            line-height: 44px;
            min-height: 44px;
            padding:0 .5em;
            margin:0;
            &.active{
            background:$headercolor;
            }
          }
        }
      }
      &.open{
        right:auto;
        left:0;
      }
    }
	}
	
	
}

/* Body block Styles */
header{
  background: adjust-color($headercolor, $lightness:+40);
  padding:0 1em;
  margin-bottom:1em;
}
.action{
  padding:1em;
  background: $green;
}


/* Typography */
@import url('http://fonts.googleapis.com/css?family=Lato:400,700,400italic');

body {
	font: 14px/150% 'Lato', Helvetica, Arial;
}
h1, h2, h3, h4, h5, h6 {
	font-family: 'Lato', Helvetica, Arial;
	font-weight: bold;
	color: $fontcolor;
}
h1 { font-size: 2em; }
h2 { font-size: 1.75em; }
h3 { font-size: 1.5em; }
h4 { font-size: 1.25em; }
h5 { font-size: 1.15em; }
h6 { font-size: 1.05em; }

/* Links */
a, a:visited { 
	color: $linkcolor;
	text-decoration:underline;
}
a:hover, a:active, a:focus {
	color: adjust-color($linkcolor, $lightness:-20);
}

/* Lists */
ul, ol
{
  li{
    margin-left:2em;
  }
}
dl{
  dt{
    margin-bottom:.25em;
  }
  dd{
    margin-bottom:.75em;
    margin-left:1em;
    &+dd{
      
      margin-top:-.5em;
    }
  }
}

nav, .radio, .checkboxes{
    ul{
    list-style-type:none;
    li{
      margin-left:0;
    }
  }
}

/* Forms */
.form{
  margin-bottom:1em;
}
fieldset {
	border: none;
	&.radio { margin: 0 0 1em; padding: 0 1em; }
	legend { background: none; }
	background:rgba(255,255,255,.2);
	&.checkboxes{
	 margin: 0 0 1em; padding: 0 1em; 
    ul{
      margin-bottom:0;
    }
	}
}
label { 
	display: inline-block;
	font-size: 1.25em;
	font-weight: bold;
	max-width:100%;
	input, select, textarea { 
		display: block;
	}
	input[type="text"], input[type="password"], input[type="date"], input[type="datetime"], input[type="datetime-local"], input[type="month"], input[type="week"], input[type="email"], input[type="number"], input[type="search"], input[type="tel"], input[type="time"], input[type="url"], textarea {
		font-size: 1.25em;
		line-height: 1.5em;
		-webkit-transition: -webkit-box-shadow 0.25s, border-color 0.25s ease-in-out;
		-moz-transition: -moz-box-shadow 0.25s, border-color 0.25s ease-in-out;
		transition: box-shadow 0.25s, border-color 0.25s ease-in-out;
	
		&:focus {
			border: 1px solid $linkcolor;
			box-shadow: 1px solid $mastheadbg;
		}
	}
	
}
.flow label{
  margin-right:1em;
  &:last-shild{
    margin-right:0;
  }
}
select{
  font-size:1.25em;
}
input[type=checkbox], input[type=radio] {
	display: inline-block;
}


button, input[type="submit"] {
	@include no_select;
	@include button_gradient($actioncolor);
	-webkit-transition: background-image 200ms ease-out;
  -moz-transition: background-image 200ms ease-out;
  transition: background-image 200ms ease-out; 
	
	font-size: 1.15em;
	min-width: 10em;
	border:none;
  -webkit-box-shadow:inset 0 -1px 1px rgba(0,0,0,.3);
  -moz-box-shadow:inset 0 -1px 1px rgba(0,0,0,.3);
  box-shadow:inset 0 -1px 1px rgba(0,0,0,.3); /*bottom internal shadow*/ 
  
  
	&.hover { @include button_gradient(adjust-color($actioncolor, $lightness:10) )}
	&:hover{@include button_gradient(adjust-color($actioncolor, $lightness:10) )}
	&.down {box-shadow: 0 0 .5em 0 #000000 inset; @include button_gradient(adjust-color($actioncolor, $lightness:-10)) }
	&:active {box-shadow: 0 0 .5em 0 #000000 inset; @include button_gradient(adjust-color($actioncolor, $lightness:-10))}
	&.disabled { background: $disabledcolor; }
	&:disabled { background: $disabledcolor; }
}

@media screen and (max-width:$tablet){
	input[type=checkbox] {
		width: 44px;
		height: 44px;
		vertical-align: middle;
		margin: 0 1em 0 0;
		&:after {
			width: 44px;
			height: 44px;
			background: #fff;
      border: 2px solid $linkcolor	;
			position: absolute;
			content: '';
		}
		&:checked:after {
			content: '\2714';
			font-size: 3em;
			color: #FFF;
			background: $actioncolor;
			text-align: center;
		}
	}
	label{
	  width:100%;
	}
	button, input[type="submit"]{
	  height:44px;
	}
}


/* Tables */
.table-scroll{
  width:100%;
  overflow-x:scroll;
  margin-bottom: 1.25em;
  table{
    margin-bottom:0;
  }
}

/* footer */

footer{
  text-align:center;
  height:5em;
  position:absolute;
  bottom:0;
  width:100%;
  nav
  {
   font-size:.75em;
    ul li{
    display:inline;
    margin:0 .5em;
    }
  }
  p{
    font-size:.75em;
  }
}


/* Modules */
.color {
	display: inline-block;
	width: 5em;
	border: 5px solid #fff;
	height: 5em;
	background: #ddd;
	box-shadow: 2px 2px 8px #999;
	margin-right: 1em;
	&.green { background: $green; }
	&.blue { background: $linkcolor; }
	&.darkgrey { background: $fontcolor; }
	&.orange { background: $orange; }
	&.lightgrey { background: $disabledcolor; }
}
.buttons {
	label { color: #999; font-weight: bold; text-align: center; }
	button { width: 100%; }
	.alternate {
		button {
			background: $actioncolor;
			font-size: 1.25em;
			display: block;
			min-width: 10em;
			border: none;
			&.hover { background: $actioncolor; }
			&.down { box-shadow: 0 0 1em #000 inset; background: $actioncolor; }
			&.disabled { background: $disabledcolor; }
		}
	}
}

/* Utilities */
label .hide{
    position:absolute;
    top:0;
    left:0;
    clip: rect(1px 1px 1px 1px); /* IE6, IE7 */ 
    clip: rect(1px, 1px, 1px, 1px); 
    display:inline;
}
.nobreak{
  white-space:nowrap;
}
@media screen and (max-width:$tablet){
  .small-hide{
    display:none !important;
  }
}
@media screen and (min-width:$tablet){
  .large-hide{
    display:none !important;
  }
}


/* Our javascript uses these to load the breakpoints. a display:none clause tells it not to use that breakpoint */
#smallBreakPoint{
  height:1px;
  width:$mobile;
}
#mediumBreakPoint{
  height:1px;
  width:$tablet;
}
#largeBreakPoint{
  display:none;
}
